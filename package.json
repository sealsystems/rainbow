    "eslint-config-seal": "0.0.9",
    "nodeenv": "3.0.63",
    "proxyquire": "2.1.3",
    "roboter": "0.14.4",
    "roboter-server": "0.14.4"
  },
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/rainbow.git"
